    </item>

    <item>
        <shape android:shape="rectangle">
            <padding android:bottom="8dp" android:top="10dp"/>
            <solid android:color="#4368DC" />
            <corners android:radius="12.0dip" />
            <stroke
                android:width="1.0dip"
                android:color="#5993fe" />
        </shape>
    </item>
</layer-list>